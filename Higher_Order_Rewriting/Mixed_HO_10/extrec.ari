(format higher-order)
(sort nat)
(fun * (-> nat nat nat))
(fun + (-> nat nat nat))
(fun O nat)
(fun rec (-> nat nat (-> nat nat nat) nat))
(fun s (-> nat nat))
(rule (+ x O) x)
(rule (+ x (s y)) (s (+ x y)))
(rule (rec O U F) U)
(rule (rec (s x) U F) (F x (rec x U F)))
(rule (* x y) (rec y O (lambda ((z1 nat) (z2 nat)) (+ x z2))))
