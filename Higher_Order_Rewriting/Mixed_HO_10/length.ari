(format higher-order)
(sort list)
(sort nat)
(fun O nat)
(fun cons (-> nat list list))
(fun foldr (-> (-> nat nat nat) nat list nat))
(fun length (-> list nat))
(fun nil list)
(fun s (-> nat nat))
(rule (foldr F x nil) x)
(rule (foldr F x (cons y ys)) (F y (foldr F x ys)))
(rule (length xs) (foldr (lambda ((x nat) (n nat)) (s n)) O xs))
