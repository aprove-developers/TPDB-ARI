(format higher-order)
(sort list)
(sort nat)
(fun + (-> nat nat nat))
(fun cons (-> nat list list))
(fun map (-> (-> nat nat) list list))
(fun nil list)
(fun ps (-> list list))
(rule (map F nil) nil)
(rule (map F (cons x xs)) (cons (F x) (map F xs)))
(rule (ps nil) nil)
(rule (ps (cons x xs)) (cons x (ps (map (lambda ((y nat)) (+ x y)) xs))))
