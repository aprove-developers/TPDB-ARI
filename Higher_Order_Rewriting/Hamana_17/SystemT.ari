(format higher-order)
(sort Nat)
(sort a)
(sort arrab)
(sort b)
(fun app (-> arrab a b))
(fun lam (-> (-> a b) arrab))
(fun rec (-> Nat a (-> Nat a a) a))
(fun succ (-> Nat Nat))
(fun zero Nat)
(rule (app (lam F) Y) (F Y))
(rule (lam (lambda ((y a)) (app U y))) U)
(rule (rec zero V I) V)
(rule (rec (succ P) X Z) (Z P (rec P X Z)))
