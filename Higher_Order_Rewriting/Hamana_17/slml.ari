(format higher-order)
(sort a)
(sort aa)
(sort ta)
(fun abs (-> (-> a a) aa))
(fun app (-> aa a a))
(fun let (-> ta (-> a ta) ta))
(fun return (-> a ta))
(rule (app (abs F) Y) (F Y))
(rule (abs (lambda ((y a)) (app U y))) U)
(rule (let (return V) I) (I V))
(rule (let P (lambda ((y a)) (return y))) P)
(rule (let (let X1 Z1) G1) (let X1 (lambda ((y a)) (let (Z1 y) G1))))
