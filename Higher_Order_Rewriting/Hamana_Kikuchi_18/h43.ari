(format higher-order)
(sort nat)
(fun O nat)
(fun rec (-> nat nat (-> nat nat nat) nat))
(fun s (-> nat nat))
(fun xplus (-> nat nat nat))
(fun xtimes (-> nat nat nat))
(rule (xplus X O) X)
(rule (xplus Y (s U)) (s (xplus Y U)))
(rule (rec O V I) V)
(rule (rec (s P) X1 Z1) (Z1 P (rec P X1 Z1)))
(rule (xtimes U1 V1) (rec V1 O (lambda ((%G nat) (%F nat)) (xplus U1 %F))))
