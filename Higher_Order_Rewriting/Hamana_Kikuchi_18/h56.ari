(format higher-order)
(sort a)
(sort b)
(fun cons (-> a b b))
(fun foldr (-> (-> a b b) b b b))
(fun nil b)
(fun xap (-> (-> a b b) a b b))
(fun yap (-> (-> b b) b b))
(rule (foldr (lambda ((%X a) (%Y b)) (yap (xap F %X) %Y)) Y nil) Y)
(rule (foldr (lambda ((%Z a) (%U b)) (yap (xap G %Z) %U)) V (cons W P)) (yap (xap G W) (foldr (lambda ((%V a) (%W b)) (yap (xap G %V) %W)) V P)))
(rule (xap F1 Y1) (F1 Y1))
(rule (yap G1 V1) (G1 V1))
