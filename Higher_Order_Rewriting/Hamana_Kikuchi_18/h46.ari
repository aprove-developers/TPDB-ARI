(format higher-order)
(sort nat)
(fun O nat)
(fun plus (-> nat nat nat))
(fun rec (-> nat nat (-> nat nat nat) nat))
(fun s (-> nat nat))
(fun succ (-> nat nat nat))
(rule (rec O X Z) X)
(rule (rec (s U) V I) (I U (rec U V I)))
(rule (succ P X1) (s X1))
(rule (plus Y1 U1) (rec Y1 U1 (lambda ((%G nat) (%F nat)) (succ %G %F))))
